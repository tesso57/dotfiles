highlight Cursor guifg=white guibg=black
set number relativenumber
set nowritebackup
set nobackup
set wrapscan
set incsearch
set hlsearch
set laststatus=2
syntax on
filetype plugin indent on
set showmatch matchtime=1
set showcmd
set nocompatible
set cursorline
set signcolumn=yes
set termguicolors
set ignorecase smartcase
set clipboard=unnamedplus
